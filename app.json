{
  "expo": {
    "name": "homekrew-user",
    "slug": "homekrew-user",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCJfWS4uoY3OPycHGr43KcmMvNiwtVJX1g"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to show nearby services and set your address."
      },
      "bundleIdentifier": "com.anmolratnpandey.homekrewuser"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCJfWS4uoY3OPycHGr43KcmMvNiwtVJX1g"
        }
      },
      "package": "com.anmolratnpandey.homekrewuser"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-maps"
    ]
  }
}